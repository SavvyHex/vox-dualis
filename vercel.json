{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY_1": "@gemini_api_key_1",
    "GEMINI_API_KEY_2": "@gemini_api_key_2",
    "GEMINI_API_KEY_3": "@gemini_api_key_3"
  }
}
